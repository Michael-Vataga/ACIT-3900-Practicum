<!DOCTYPE html>
<html lang="en">
    {{> head}}
<body style="height: 66.7vh;">
    {{> menu}}
    <br><br>
    <div class="container">
        <div class="grid-container">
            <h1 style="width: 80%; margin: 0 auto">{{heading}}</h1>
            <div style="width:80%; margin: 0 auto">
                <table class="table">
                    <tr>
                        <th style="width: 20%">Time</th>
                        <th style="width: 80%">Description</th>
                    </tr>
                    {{#each agendaItems}}
                        <tr>
                            <td>{{timeStart}} to {{timeEnd}}</td>
                            <td>{{description}}</td>
                        </tr>
                    {{/each}}
                </table>
            </div>
        </div>
    </div>
</body>
{{> footer}}
</html>